          "patterns": [
            {
              "name": "string.quoted.csound",
              "begin": "\"",
              "end": "\"",
              "beginCaptures": {
                "0": {
                  "name": "punctuation.definition.string.begin.csound"
                }
              },
              "endCaptures": {
                "0": {
                  "name": "punctuation.definition.string.end.csound"
                }
